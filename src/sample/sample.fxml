<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Rectangle?>
<?import java.net.URL?>
<GridPane id="buttonBox01" alignment="TOP_LEFT" hgap="10.0" prefHeight="200.0" prefWidth="500.0" vgap="10.0"
          xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="sample.Controller">
    <children>
        <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0" GridPane.columnIndex="0"
              GridPane.rowIndex="0">
            <children>
                <VBox id="VBox" alignment="CENTER" spacing="5.0">
                    <children>
                        <Label fx:id="labelBox01" text="Box 01"/>
                        <Rectangle id="box01" fx:id="rectangleBox01" arcHeight="5.0" arcWidth="5.0" fill="#ebff99"
                                   height="100.0" stroke="BLACK" strokeType="INSIDE" width="100.0"/>
                        <Button fx:id="buttonBox01" minHeight="100.0" minWidth="100.0" mnemonicParsing="false"
                                onAction="#onButtonBox01Click" text="Reserve">
                            <stylesheets>
                                <URL value="@styles.css"/>
                            </stylesheets>
                        </Button>
                    </children>
                </VBox>
                <VBox id="VBox" alignment="CENTER" spacing="5.0">
                    <children>
                        <Label fx:id="labelBox02" text="Box 02"/>
                        <Rectangle id="box02" fx:id="rectangleBox02" arcHeight="5.0" arcWidth="5.0" fill="#14ff00"
                                   height="100.0" stroke="BLACK" strokeType="INSIDE" width="100.0"/>
                        <Button fx:id="buttonBox02" minHeight="100.0" minWidth="100.0" mnemonicParsing="false"
                                onAction="#onButtonBox02Click" text="Reserve">
                            <stylesheets>
                                <URL value="@styles.css"/>
                            </stylesheets>
                        </Button>
                    </children>
                </VBox>
                <VBox id="VBox" alignment="CENTER" spacing="5.0">
                    <children>
                        <Label fx:id="labelBox03" text="Box 03"/>
                        <Rectangle id="box03" fx:id="rectangleBox03" arcHeight="5.0" arcWidth="5.0" fill="#66c8ff"
                                   height="100.0" stroke="BLACK" strokeType="INSIDE" width="100.0"/>
                        <Button fx:id="buttonBox03" minHeight="100.0" minWidth="100.0" mnemonicParsing="false"
                                onAction="#onButtonBox03Click" text="Reserve">
                            <stylesheets>
                                <URL value="@styles.css"/>
                            </stylesheets>
                        </Button>
                    </children>
                </VBox>
            </children>
        </HBox>
    </children>
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
    </rowConstraints>
</GridPane>
